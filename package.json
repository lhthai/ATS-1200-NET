{
  "name": "server",
  "version": "1.0.0",
  "description": "Software for truck scale system ATS-1200-NET",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node server/server.js",
    "build": "babel ./server -d ./build",
    "start": "nodemon --exec babel-node server/server.js",
    "test": "mocha --require @babel/register server/test/settings/*.js"
  },
  "nodemonConfig": {
    "ignore": [
      "src/upload/*",
      "build/storage/*",
      "client/*"
    ]
  },
  "keywords": [
    "ATS-1200-NET",
    "truck-scale-software",
    "Tanaka-scale-vietnam",
    "TSV",
    "Tanaka-scale"
  ],
  "author": "Le Hong Thai",
  "license": "MIT",
  "dependencies": {
    "@serialport/parser-readline": "^8.0.6",
    "aws-sdk": "^2.622.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "drivelist": "^8.0.10",
    "escpos": "^2.5.2",
    "express": "^4.17.1",
    "mongoose": "^5.7.5",
    "serialport": "^8.0.7",
    "socket.io": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
